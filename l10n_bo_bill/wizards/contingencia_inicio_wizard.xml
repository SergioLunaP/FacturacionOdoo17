<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_contingencia_inicio_wizard_form" model="ir.ui.view">
        <field name="name">contingencia.inicio.wizard.form</field>
        <field name="model">contingencia.inicio.wizard</field>
        <field name="arch" type="xml">
            <form string="Confirmar Contingencia">
                <group>
                    <field name="codigo_evento"/>
                    <field name="descripcion"/>
                </group>
                <footer>
                    <button name="confirmar_contingencia" string="Confirmar" type="object" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
    
</odoo>